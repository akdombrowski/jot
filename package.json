{
  "name": "jwt-authn",
  "version": "1.0.31",
  "description": "JWT decoder, encoder, and more",
  "main": "./lib",
  "type": "module",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "test": "cross-env NODE_ENV=test nyc mocha test",
